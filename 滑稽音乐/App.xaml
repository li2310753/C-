<Application x:Class="滑稽音乐.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:滑稽音乐"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="DefaultStyles.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <ImageBrush x:Key="关闭按钮" ImageSource="E:\Important\Picture\图标\关闭.png" />
            <ImageBrush x:Key="最大化按钮" ImageSource="E:\Important\Picture\图标\最大化.png" />
            <ImageBrush x:Key="最小化按钮" ImageSource="E:\Important\Picture\图标\最小化.png" />
            <ImageBrush x:Key="还原按钮" ImageSource="E:\Important\Picture\图标\还原.png" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
